{
  "name": "makeup-store",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next:lint": "next lint",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:4000/graphql schema.gql",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "dotenv": "^16.0.3",
    "graphql": "^15.8.0",
    "lodash.xor": "^4.5.0",
    "next": "11.1.2",
    "next-pwa": "^5.4.7",
    "next-seo": "^5.5.0",
    "nextjs-progressbar": "^0.0.14",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-simple-star-rating": "^3.0.0",
    "react-slick": "^0.29.0",
    "sharp": "^0.30.6",
    "styled-components": "5.3.6",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/preset-typescript": "^7.15.0",
    "@storybook/addon-essentials": "6.5.8",
    "@storybook/react": "6.3.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^29.2.3",
    "@types/lodash.xor": "^4.5.6",
    "@types/node": "18.8.2",
    "@types/react": "^17.0.39",
    "@types/react-slick": "^0.23.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "apollo": "^2.33.10",
    "babel-loader": "^9.0.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^7.0.2",
    "jest": "^29.3.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.4.2",
    "plop": "^3.1.1",
    "prettier": "^2.8.0",
    "storybook-addon-next-router": "^3.0.8",
    "typescript": "^4.7.4"
  }
}
